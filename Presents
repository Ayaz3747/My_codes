import java.util.Scanner;
public class Presents {
    public static void main(String []a){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int[] num = new int[n];
        for(int i=0; i<n; i++){
            num[i] = sc.nextInt();
        }
        for(int i=0; i<n; i++){
        for(int j=0; j<n; j++){
            if((num[j]-1)==i) {
                System.out.print((j + 1)+" ");
                break;
            }
            }
        }
    }
}
